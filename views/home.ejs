<%- include("header"); -%> 

    <div class="container">
      
        <nav class="navbar navbar-expand-lg">
            <div class="container-fluid">
              <a class="navbar-brand logo mx-3" href="#"><img src="/images/favicon.png" alt=""></a>
              <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
              <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                  <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="#">Home</a>
                  </li>
                </ul>
                <form action="/home" method="post">
                  <a href="/route/logout" class="btn btn-outline-primary">Logout</a>
                  <!-- <button class="btn btn-outline-primary" type="submit">Logout</button> -->
                </form>
              </div>
            </div>
          </nav>

        <% for(let i=0,k=0; i<3; i++){ %> 
        <div class="row">
           <% for(let j=0; j<4; j++,k++){ %>  
            <div class="col-sm-6 col-md-3 col-6 mt-4">
                <div class="card">
                    <img src="<%= apes[k].imgUrl %>" class="card-img-top" alt="...">
                    <div class="card-body">
                      <p class="card-text">model: <%= apes[k].model %> </p>
                      <p class="card-text">price: <%= apes[k].price %> </p>
                      <a href="#" class="btn btn-primary">Buy now</a>
                    </div>
                  </div>
            </div>
           <% } %>  
        </div>
        <% } %> 
    </div>

<%- include("footer"); -%> 